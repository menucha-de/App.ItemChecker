<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="havis.app.itemchecker.ui.resourcebundle.AppResources" field="res"></ui:with>
	<ui:style>
	
	.label{
		width: 15%;		
		font-weight:bold;
		display: inline-block;
		vertical-align:top;
		font-size:1.25em;
		line-height:1.5em;
		text-align:center;
	}
	</ui:style>
	
	<g:FlowPanel addStyleNames="{res.css.progress}">
		<g:FlowPanel ui:field="bar" addStyleNames="{res.css.progressbar}">
		</g:FlowPanel>
		<g:Label ui:field="progressLabel" addStyleNames="{style.label}">
		</g:Label>
	</g:FlowPanel>	
</ui:UiBinder> 