<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="havis.app.itemchecker">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="elemental.Elemental" />
	<inherits name="org.fusesource.restygwt.RestyGWT" />
	<inherits name="havis.application.HAL" />	

	<inherits name="havis.app.itemchecker.ItemChecker" />
	<inherits name="havis.net.rest.shared.Shared" />
	<inherits name="havis.net.ui.shared.Shared" />
	<inherits name="havis.net.ui.core.theme.macio.Macio" />
	<inherits name="com.google.gwt.i18n.I18N" />

	<set-property name="user.agent" value="safari" />

	<source path="resourcebundle" />
	<source path="handheld" />
	<source path="client/custom" />

	<entry-point class="havis.app.itemchecker.ui.handheld.WebUIHandheld" />
</module>